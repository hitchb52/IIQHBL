<!--
  Initialization file for SailPoint Access History, this enables Access History and dependencies.
-->
<sailpoint>
  <ImportAction name="merge">
    <Configuration name="SystemConfiguration">
      <Attributes>
        <Map>
          <entry key="accessHistoryEnabled" value="true" />
          <entry key="messageBusEnabled" value="true" />
          <entry key="enableInterceptedDeleteCapture" value="true" />
        </Map>
      </Attributes>
    </Configuration>
  </ImportAction>
</sailpoint>
