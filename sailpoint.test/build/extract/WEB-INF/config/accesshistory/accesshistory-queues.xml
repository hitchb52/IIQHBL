<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
  <ImportAction name="merge">
    <Configuration name="AccessHistoryConfiguration">
      <Attributes>
        <Map>
          <entry key="internalProcessingQueue" value="queue://accessHistoryProcessingQueue" />
          <entry key="messageTimeToLiveInMinutes" value="60" />
          <!-- destinationQueue is referenced by DataExtract -->
          <entry key="destinationQueue" value="queue://accessHistoryExtractedObjects" />
        </Map>
      </Attributes>
    </Configuration>
  </ImportAction>
</sailpoint>
